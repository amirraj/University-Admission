


@{
    Layout = null;
}
@if (Session["adminlogin"] == null)
{
    Response.Redirect("~/Login/Index");
}
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Online Admission - Admin</title>
    <link href="~/Content/Home/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <nav class="navbar fixed-top navbar-expand-lg navbar-dark bg-dark fixed-top">
        <div class="container">
            <a class="navbar-brand" href="@Url.Action("Index", "Home")">Online Admission</a>
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Logout", "Login")">Logout</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <div class="container">
        <br /><br /><br /> <br /> <br />
        <div class="card" style="width: 55rem;">
            <div class="card-body">

                <div class="row">
                    <div class="col-md-6">

                    </div>
                    <div class="col-md-6">
                        <button type="button" class="btn btn-primary">Search Student</button>
                    </div>
                </div>

                <br />

                <div class="row">
                    <div class="col-md-6">
                        Year: @Html.DropDownList("Year!", ViewBag.year as SelectList, "Select  Year")
                    </div>
                    <div class="col-md-6">
                        <button type="button" class="btn btn-primary">Search Question</button>
                    </div>
                </div>
                <br />

                <div class="row">
                    <div class="col-md-6">

                    </div>
                    <div class="col-md-6">
                        <button type="button" class="btn btn-primary">Search Result</button>
                    </div>
                </div>
                <br />

                <div class="row">
                    <div class="col-md-6">
                        @*<button type="button" class="btn btn-primary"> <a href="@Url.Action("SetExam", "Admin")">Set Exam</a> </button>*@
                        <a class="btn btn-primary" href="@Url.Action("SetExam", "Admin")">Set Exam</a>
                    </div>
                    <div class="col-md-6">
                        <a class="btn btn-primary" href="@Url.Action("AssignSetter", "Admin")">Assign Setter</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br /> <br /> <br /> <br /> <br /> <br /> <br /> <br /> <br />  <br />
    <footer class="py-5 bg-dark">
        <div class="container">
            <p class="m-0 text-center text-white">Copyright &copy; Your Website 2019</p>
        </div>
        <!-- /.container -->
    </footer>
    <script src="~/Content/Home/vendor/jquery/jquery.min.js"></script>
</body>
</html>

